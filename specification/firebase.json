{
  "hosting": {
    "public": "firebase",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "pr/:pr/DartLangSpecDraft.pdf",
        "destination": "https://storage.googleapis.com/dart-specification/pr/:pr/DartLangSpecDraft.pdf",
        "type": 301
      },
      {
        "source": "**",
        "destination": "https://dart.dev/resources/language/spec",
        "type": 302
      }
    ]
  }
}
